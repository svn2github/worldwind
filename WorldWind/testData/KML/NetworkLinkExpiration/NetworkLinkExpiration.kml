<?xml version="1.0" encoding="UTF-8"?>
<!-- Test Suite for remote NetworkLinks. Open this document in KMLViewer. The label on each placemark should change
     every 5 seconds. -->
<!--$Id$-->
<kml xmlns="http://www.opengis.net/kml/2.2">
    <Document>
        <name>NetworkLink Expiration Test Suite</name>
        <description>The label of each placemark should update every 5 seconds.</description>
        <open>1</open>

        <NetworkLink>
            <name>Expires</name>
            <open>1</open>
            <Link>
                <href>
                    http://pabercrombie.com/ww/kml/NetworkLinkExpiration_expires.php
                </href>
                <refreshMode>onExpire</refreshMode>
            </Link>
        </NetworkLink>

        <NetworkLink>
            <name>Cache-Control</name>
            <open>1</open>
            <Link>
                <href>
                    http://pabercrombie.com/ww/kml/NetworkLinkExpiration_max_age.php
                </href>
                <refreshMode>onExpire</refreshMode>
            </Link>
        </NetworkLink>

        <NetworkLink>
            <name>Expires and Cache-Control</name>
            <open>1</open>
            <Link>
                <href>
                    http://pabercrombie.com/ww/kml/NetworkLinkExpiration_expires_and_max_age.php
                </href>
                <refreshMode>onExpire</refreshMode>
            </Link>
        </NetworkLink>

    </Document>
</kml>